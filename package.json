{
  "name": "go-seven.com",
  "version": "0.0.0",
  "private": true,
  "description": "URL shortener",
  "scripts": {
    "aws_s3_sync": "aws s3 sync public s3://go-seven.com --profile GoSeven",
    "browserify": "cross-env NODE_ENV=production browserify src/index.tsx -p tsify -g [ envify --NODE_ENV production ] -g uglifyify | uglifyjs --compress --mangle > public/bundle.js",
    "build": "npm run browserify; npm run node-sass",
    "budo": "budo src/index.tsx --live --pushstate --dir public --serve bundle.js --open -- -p tsify",
    "deploy": "npm run build; npm run aws_s3_sync",
    "node-sass": "node-sass --include-path node_modules src/sass/style.scss --output-style compressed --output public/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc--noemit": "tsc --declaration --project . --noemit",
    "tslint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "pre-commit": [
    "tsc--noemit",
    "tslint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/go-seven/go-seven.com.git"
  },
  "keywords": [
    "go7"
  ],
  "author": "Gianluca Casati (http://g14n.info)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/go-seven/go-seven.com/issues"
  },
  "homepage": "https://go7.li",
  "dependencies": {
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "budo": "^11.6.0",
    "bulma": "^0.7.2",
    "cross-env": "^5.2.0",
    "dot-editorconfig": "^1.1.1",
    "envify": "^4.1.0",
    "fa-svg-icon": "^0.3.0",
    "i-am-not-a-robot": "^0.6.0",
    "node-sass": "^4.11.0",
    "pdsp": "^0.1.0",
    "pre-commit": "^1.2.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "trunx": "^0.18.2",
    "tsify": "^4.0.1",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "uglify-es": "^3.3.9",
    "uglifyify": "^5.0.1"
  },
  "devDependencies": {}
}
